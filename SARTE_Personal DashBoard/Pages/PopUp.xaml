<Window x:Class="SARTE_Personal_DashBoard.Pages.PopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SARTE_Personal_DashBoard.Pages"
        mc:Ignorable="d"
        Title="PopUp" Height="500" Width="700" WindowStartupLocation="CenterScreen">
    <Grid  Background="#FFF1EDE3">
        <TextBlock x:Name="tbArtworkName" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="35" Margin="407,151,0,0" Width="227" Text="Artwork Name" IsEnabled="False" Foreground="#FF4C2306" FontFamily="Ink Free"/>
        <Image x:Name="imgArt" HorizontalAlignment="Left" Height="382" Margin="21,46,0,0" VerticalAlignment="Top" Width="410"/>
        <Button Content="I'm Interested!" HorizontalAlignment="Left" Margin="407,273,0,0" VerticalAlignment="Top" Width="227" Background="#FFD8AD8F" RenderTransformOrigin="1.014,-0.856" Height="43" FontWeight="Bold" Click="Button_Click"/>
        <TextBlock x:Name="tbArtistName" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="20" Margin="407,198,0,0" Width="227" Text="@elleekabook" Foreground="#FF4C2306"/>
        <TextBlock x:Name="tbPrice" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="20" Margin="407,231,0,0" Width="227" Text="Price" Foreground="#FF4C2306"/>
    </Grid>
</Window>
